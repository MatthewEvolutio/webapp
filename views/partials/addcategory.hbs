<form class="ui stacked segment form" action="/dashboard/addcategory" method="POST">
 <div class="one field">
  <div class="field">
    <label>Category Name</label>
    <input placeholder="Category" type="text" name="category" required>
  </div>
  <div class="field">
    <label>Rating</label>
    <input type="number" name="rating" min="1" max="5" required>
  </div>
 </div>
 <button class="ui blue submit button">Add Category</button>
</form>
